{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": 0,
        "comma-dangle": 0,
        "no-console": "off",
        "newline-per-chained-call": "off", // TODO: Check for this in Prettier
        "no-underscore-dangle": "off"
    }
}
